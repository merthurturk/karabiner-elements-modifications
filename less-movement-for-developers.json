{
  "title": "Software development $/{/}/(/)/=/-/->/=>/enter/backspace shortcuts",
  "rules": [
    {
      "description": "capslock for left alt control gui and shift",
      "manipulators": [
        {
          "description": "Change caps_lock to command+control+option+shift.",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_shift",
              "modifiers": [
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "caps_lock + ijkl for arrow keys",
      "manipulators": [
        {
          "description": "Use caps_lock + J as left arrow key",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_alt",
                "left_control",
                "left_gui",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "Use caps_lock + L as left arrow key",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "left_alt",
                "left_control",
                "left_gui",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "Use caps_lock + I as up arrow key",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "left_alt",
                "left_control",
                "left_gui",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "Use caps_lock + K as up arrow key",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_alt",
                "left_control",
                "left_gui",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "caps_lock + ; for enter",
      "manipulators": [
        {
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": [
                "left_alt",
                "left_control",
                "left_gui",
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
